#include<stdio.h>
int main(){
int gasto;
printf("ingrese el gasto de corriente\n");
scanf("%i\n",&gasto);
if(gasto<1000){
    printf("tiene que pagar %i\n",(gasto*1.2));
}else{
    if(gasto=1000,1850){
        printf("tiene que pagar %i\n",(gasto*1.8));
    }else{
        if(gasto>1859){
            printf("tiene que pagar %i\n",(gasto*2.1));
        }
    }
}
return 0;
    
}
