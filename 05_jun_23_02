int main(){
	int opcion;
	scanf("%i\n",opcion);
	switch(opcion)
	{
	case 0:
		printf("menor de tres....\n");
		break;
    case 1:
    	printf("menor de tres....\n");
    	break;
    case 2:
       printf("menor de tres....\n");
    break;
    case 3:
    	printf("igual a 3\n");
    	break;
    default:
    	printf("fuera de rango\n");
	     
		
	}
}
