#include<stdio.h>
int main()
{
	int m=45,n=75;
	printf("m=%d, n=%d, \n",m,n);/*nueva asignacion de cadena de cntro %significa imprimir, "d" de que tipo es*/
	++m;
	--n;
	printf("m=%d, n=%d\n",m,n);
	m++;
	n--;
	printf("m=%d, n=%d, \n",m,n);
}
